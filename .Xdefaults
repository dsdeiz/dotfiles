! ---------------------------------------------------------
! URxvt Settings
! ---------------------------------------------------------

URxvt*font: xft:Monaco:size=7.5
URxvt*boldFont: xft:Monaco:size=7.5

URxvt*transparent: 1
URxvt*shading: 4
URxvt*scrollBar: false
URxvt*termName: rxvt-256color
URxvt*cursorBlink: 1
URxvt*cursorColor: #e8621d

URxvt.urgentOnBell: 1

URxvt*perl-ext-common:  default,matcher
URxvt*urlLauncher:      /usr/bin/google-chrome
URxvt*matcher.button:   1

URxvt*background: #000000
URxvt*foreground: #ffffff
! Black
URxvt*color0 : #000000
URxvt*color8 : #555753
! Red
URxvt*color1 : #bf2d00
URxvt*color9 : #dd4010
! Green
URxvt*color2 : #5b9f1e
URxvt*color10: #93d44f
! Yellow
URxvt*color3 : #eab93d
URxvt*color11: #ffc123
! Blue
URxvt*color4 : #204a87
URxvt*color12: #3465a4
! Magenta
URxvt*color5 : #d6156c
URxvt*color13: #fe4365
! Cyan
URxvt*color6 : #619bd7
URxvt*color14: #89b6e2
! White
URxvt*color7 : #ffffff
URxvt*color15: #dddddd

