! ---------------------------------------------------------
! URxvt Settings
! ---------------------------------------------------------

URxvt.font: xft:Monaco:size=7.5
URxvt.boldFont: xft:Monaco:size=7.5

URxvt*transparent: 1
URxvt*shading: 10
URxvt*scrollBar: false
URxvt*termName: rxvt-256color
URxvt*cursorBlink: 1
URxvt*cursorColor: #e8621d

URxvt.urgentOnBell: 1

URxvt*perl-ext-common:  default,matcher
URxvt*urlLauncher:      /usr/bin/google-chrome
URxvt*matcher.button:   1

! IR_Black
URxvt*background:   #000000
URxvt*foreground:   #DCDCCC
! black
URxvt*color0:       #4E4E4E
URxvt*color8:       #7C7C7C
!red
URxvt*color1:       #FF6C60
URxvt*color9:       #FFB6B0
!green
URxvt*color2:       #A8FF60
URxvt*color10:      #CEFFAB
!yellow
URxvt*color3:       #FFFFB6
URxvt*color11:      #FFFFCB
!blue
URxvt*color4:       #96CBFE
URxvt*color12:      #FFFFCB
!magenta
URxvt*color5:       #FF73FD
URxvt*color13:      #FF9CFE
!cyan
URxvt*color6:       #C6C5FE
URxvt*color14:      #DFDFFE
!white
URxvt*color7:       #EEEEEE
URxvt*color15:      #FFFFFF

! ---------------------------------------------------------
! Vim Settings
! ---------------------------------------------------------

Vim*font: xft:Monaco:size=10pt
Vim*boldFont: xft:Monaco:bold:size=10pt
# Vim*transparent: 0

! ---------------------------------------------------------
! Xft Settings
! ---------------------------------------------------------

Xft.autohint:             1
Xft.lcdfilter:            lcddefault
Xft.hintstyle:            hintslight
Xft.hinting:              1
Xft.rgba:                 rgb
Xft.antialias:            1

