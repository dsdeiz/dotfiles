! ---------------------------------------------------------
! URxvt Settings
! ---------------------------------------------------------

URxvt*font: -*-terminus-*-*-*--*-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-*-*-*--*-*-*-*-*-*-*-*

URxvt*transparent: 1
URxvt*shading: 8
URxvt*scrollBar: false
URxvt*termName: rxvt-256color
URxvt*cursorBlink: 1
URxvt*cursorColor: #e8621d

Xft.autohint:             1
Xft.lcdfilter:            lcdlight
Xft.hintstyle:            hintslight
Xft.hinting:              1
Xft.antialias:            0

URxvt*perl-ext-common:  default,matcher
URxvt*urlLauncher:      /usr/bin/google-chrome
URxvt*matcher.button:   1 

URxvt*foreground:            #dddddd
URxvt*background:           #151515
!black
URxvt*color0:                   #393939
URxvt*color8:                   #474747
!red
URxvt*color1:                   #da4939
URxvt*color9:                   #ff6c5c
!green
URxvt*color2:                   #519f50
URxvt*color10:                 #a5c261
!yellow
URxvt*color3:                   #cc7833
URxvt*color11:                  #bc9458
!blue
URxvt*color4:                   #6d9cbe
URxvt*color12:                 #d0d0ff
!magenta
URxvt*color5:                   #9f5079
URxvt*color13:                 #a761c2
!cyan
URxvt*color6:                   #435d75
URxvt*color14:                 #6e98a4
!white
URxvt*color7:                   #dddddd
URxvt*color15:                 #dddddd
