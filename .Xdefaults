! ---------------------------------------------------------
! URxvt Settings
! ---------------------------------------------------------

URxvt*font: xft:Monaco:style=Regular:size=7.5
URxvt*boldFont: xft:Monaco:style=Regular:size=7.5

URxvt*transparent: 1
URxvt*shading: 20
URxvt*scrollBar: false
URxvt*termName: rxvt-256color

Xft.autohint:             1
Xft.lcdfilter:            lcdlight
Xft.hintstyle:            hintslight
Xft.hinting:              1
Xft.antialias:            0
Xft.dpi:                  96
Xft.rgba:                 rgb

URxvt*perl-ext-common:  default,matcher
URxvt*urlLauncher:      /usr/bin/google-chrome
URxvt*matcher.button:   1 

!Technobass - Foreground/Background
URxvt*foreground: #b4b4b4
URxvt*background: #121212

!Technobass
! -- black
URxvt*color0: #000000
! -- red
URxvt*color1: #ff8888
! -- green
URxvt*color2:  #80852d
! -- yellow
URxvt*color3: #ffaa55
! -- blue
URxvt*color4: #82b8f2
! -- purple
URxvt*color5: #ad7fa8
! -- cyan
URxvt*color6: #6b9b8b
! -- white
URxvt*color7: #babdb6
! -- bright-black
URxvt*color8: #555753
! -- bright-red
URxvt*color9: #e5505f
! -- bright-green
URxvt*color10: #b7e234
! -- bright-yellow
URxvt*color11: #e8621d
! -- bright-blue
URxvt*color12:  #0088cc
! -- bright-purple
URxvt*color13: #cb99e1
! -- bright-cyan
URxvt*color14: #34E2E2
! -- bright-white
URxvt*color15: #e9e9e9
